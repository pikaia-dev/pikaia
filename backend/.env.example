# Local Development Environment
# Copy to .env and fill in values

# Django
SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/tango

# Stytch (https://stytch.com/dashboard)
STYTCH_PROJECT_ID=
STYTCH_SECRET=
STYTCH_WEBHOOK_SECRET=  # Svix signing secret for webhooks

# Stripe (https://dashboard.stripe.com/test/apikeys)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_ID=

# Resend (https://resend.com/api-keys)
RESEND_API_KEY=

# WebAuthn (Passkeys)
WEBAUTHN_RP_ID=localhost
WEBAUTHN_RP_NAME=Tango B2B
WEBAUTHN_ORIGIN=http://localhost:5173

# Stytch Trusted Auth Token (for passkey -> Stytch session)
# Create profile at https://stytch.com/dashboard/trusted-auth-tokens
STYTCH_TRUSTED_AUTH_PROFILE_ID=
STYTCH_TRUSTED_AUTH_AUDIENCE=stytch
STYTCH_TRUSTED_AUTH_ISSUER=passkey-auth
PASSKEY_JWT_PRIVATE_KEY=  # RSA private key (PEM format, newlines as \n)

# CORS (production only - comma-separated URLs)
# CORS_ALLOWED_ORIGINS=https://app.example.com,https://www.example.com
